{
    "manifest_version": 3,
    "name": "Krabs Dev",
    "version": "1.0.0",
    "action": {
        "default_icon": {},
        "default_title": "Krabs - Be eaten by me"
    },
    "icons": {
        "48": "images/krabs.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/sleeper.js"],
            "matches": ["https://*/*"]
        }
    ],
    "background": {
        "service_worker": "sw.js",
        "type": "module"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "contextMenus",
        "storage",
        "identity"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    },
    "key": "",
    "web_accessible_resources": [
        {
            "resources": ["images/*", "assets/*", "scripts/*"],
            "matches": ["https://*/*"]
        }
    ]
}
