{
    "manifest_version": 3,
    "name": "Pocket Drive",
    "version": "1.0.0",
    "action": {
        "default_icon": {},
        "default_title": "Pocket Drive - save images to google drive"
    },
    "icons": {
        "48": "images/krabs.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content/sleeper.js"],
            "matches": ["https://*/*"]
        }
    ],
    "minimum_chrome_version": "93",
    "side_panel": {
        "default_path": "scripts/sidepanel/sidepanel.html"
    },
    "background": {
        "service_worker": "scripts/worker/sw.js",
        "type": "module"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "contextMenus",
        "storage",
        "identity",
        "sidePanel"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+B",
                "mac": "Command+B"
            }
        }
    },
    "key": "",
    "web_accessible_resources": [
        {
            "resources": ["images/*", "assets/*", "scripts/content/*"],
            "matches": ["https://*/*"]
        }
    ]
}
